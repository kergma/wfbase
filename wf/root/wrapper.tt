<body>

[% IF c.check_any_user_role('разработчик') %]
<div style="text-align: right; width: 100%">
	<div style="text-align:right;color:lightgray">[% version %]</div>
</div>

[% END %]

<div><!-- main-menu -->
	<div id="auth">
		<div id="login-name" title="[% IF c.user.full_name %][% c.user.full_name %][% END %][% IF c.user.roles %], &#10;[% c.user.roles.join(', ') %][% END %], &#10; [% c.user.spname.join('; ') %]">[% c.user.username %] </div>
		<div id="logout"><a href="[% c.uri_for('/auth/logout') %]">выход</a></div>
	</div>
	<ul id="main-menu">
		[% IF c.check_any_user_role('отправляющий','разработчик') %]
		<li><a href="#">Загрузка</a>
			<ul>
				<li><a href="/upload/packet">Загрузить пакет</a></li>
			</ul>
		</li>
		[% END %]
		<li><a href="#">Просмотр</a>
			[% IF c.check_any_user_role('оператор','диспетчер','администратор') %]
			<ul>
				<li><a href="[% c.uri_for('/search/objects') %]">Объекты</a></li>
				<li><a href="[% c.uri_for('/search/orders') %]">Заказы</a></li>
				<li><a href="[% c.uri_for('/search/packets') %]">Пакеты</a></li>
				<li><a href="[% c.uri_for('/search/events') %]">События</a></li>
				<li><a href="/query/cat">Каталог</a></li>
			</ul>
			[% END %]
		</li>

		[% IF c.check_any_user_role('оператор','диспетчер','администратор') %]	
		<li><a href="#">Добавить</a>
			<ul>
				<li><a href="/edit/insert/objects?source=web">Объект</a></li>
				<li><a href="/edit/insert/orders">Заказ</a></li>
				<li><a href="/edit/insert/packets">Пакет</a></li>
				<li><a href="/edit/insert/log_old?who=[% c.user.username %]">Событие</a></li>
			</ul>
		</li>
		<li><a href="#">Обработка</a>
			<ul>
				<li><a href="[% c.uri_for('/log/browse') %]">Просмотр журнала</a></li>
			</ul>
		</li>
		[% END %]

		[% IF c.check_any_user_role('администратор','разработчик') %]
		<li><a href="#">Управление</a>
			<ul>
				[% IF c.check_any_user_role('администратор','разработчик') %]
				<li><a href="[% c.uri_for('/export/status') %]">Передача пакетов</a></li>
				<li><a href="[% c.uri_for('/export/history') %]">История передач</a></li>
				[% END %]
				<li><a href="[% c.uri_for('/dispatch/queue') %]">Очередь обработки</a></li>
				[% IF c.check_any_user_role('контролирующий','разработчик') %]
				<li><a href="/query/edit">Редактор</a></li>
				<li><a href="/task/status">Текущие задачи</a>
					<ul>
						<li><a href="/task/schedule?задача=передача запросов сведений об окс&confirm=1">Передать запросы сведений об ОКС</a></li>
					</ul>
				</li>
			</ul>
		</li>
		[% END %]
	[% END %]

		<li><a href="#">Помощь</a>
			<ul>
				<li><a href="http://trac.sfo.rosinv.ru/wiki/Новости">Новости</a></li>
				<li><a href="http://trac.sfo.rosinv.ru/wiki/Инструкции">Инструкции</a></li>
				<li><a href="http://trac.sfo.rosinv.ru/newticket">Сообщить об ошибке</a></li>
				<li><a href="/storage/file/1e202006-1369-8941-ae73-bf7299eab5be">Техподдержка</a></li>
			</ul>
		</li>
	</ul>
</div> <!-- end menu -->

<div id="header">
    [%# Insert the page title -%]
    <!--<h1>[% template.title or site.title %]</h1>-->
    <h1> [% heading or template.title or site.title %]</h1>
</div>

<div id="content">
    [% IF error_msg %]<span class="error">[% error_msg %]</span>[% END %]
    [%# This is where TT will stick all of your template's contents. -%]
    [% content %]
[% IF c.check_user_roles('разработчик') %]
<pre>
[% result.dump %]
[% dump %]
</pre>
[% END %][%# check_user_roles('разработчик') %]
</div><!-- end content -->

<div id="footer">
	<br><img src="/static/images/rosinv-logo.png"/>
</div>

</body>
</html>
