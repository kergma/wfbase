[%# vim: set filetype=tt2: -%]
[%UNLESS footer%]
[%IF n.title%]<div>[%n.title%][%IF c.check_any_user_role('разработчик')%] ([%n.key%])[%END%]</div>[%END%]
[%END-%]

[%h=n.value%]
[%IF h.form%]
	[%INCLUDE form.tt form=h.form footer=footer%]
[%ELSIF n.key=="filelist"%]
	[%INCLUDE filelist.tt filelist=n.value footer=footer%]
	[%n.stop=1%]
[%ELSIF n.key=="uploader"%]
	[%INCLUDE uploader.tt UNLESS footer%]
	[%n.stop=1%]
[%ELSIF h.query%]
	[%INCLUDE retrieval.tt h=h UNLESS footer%]
	[%INCLUDE rowser.tt h=h UNLESS footer%]
	[%n.stop=1%]
[%ELSE%]
	[%UNLESS footer%]
		[%-IF h.text %]<p>[%h.text%]</p>[%END%]
	[%ELSE%]
	[%END%]
[%END-%]

[%IF footer%]
[%END%]

